{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    {
      "VerifiableId": {
        "@id": "https://eu-digital-identity-wallet.github.io/eudi-doc-architecture-and-reference-framework/1.4.0/annexes/annex-3/annex-3.01-pid-rulebook/",
        "@context": {
          "@version": 1.1,
          "@protected": true,
          "schema": "https://schema.org/",
          "id": "@id",
          "type": "@type",
          "given_name": "schema:givenName",
          "family_name": "schema:familyName",
          "resident_address": "schema:address",
          "resident_country": "schema:country",
          "gender": "schema:gender",
          "birth_date": "schema:birthDate",
          "birth_place": "schema:birthPlace",
          "personalIdentifier": "schema:identifier",
          "age_over_12": "schema:age",
          "age_over_14": "schema:age",
          "age_over_16": "schema:age",
          "age_over_18": "schema:age",
          "age_over_21": "schema:age",
          "age_over_65": "schema:age",
          "issuance_date": "schema:date"
        }
      },
      "DocumentVerification": {
        "@id": "https://github.com/TalaoDAO/context#VerificationMethod.jsonld",
        "@context": {
          "@version": 1.1,
          "@protected": true,
          "schema": "https://schema.org/",
          "id": "@id",
          "type": "@type",
          "verifier": "schema:legalName",
          "verificationMethod": "schema:identifier",
          "dossier" : "schema:identifie",
          "parcours" : "schema:identifier",
          "levelOfAssurance": "https://ec.europa.eu/digital-building-blocks/wikis/display/DIGITAL/eIDAS+Levels+of+Assurance"
        }
      }
    }
  ],
  "id": "",
  "type": [
    "VerifiableCredential",
    "VerifiableId"
  ],
  "credentialSubject": {
    "type": "VerifiableId"
  },
  "evidence": [
    {
      "id": "urn:id360:<dossier_id>",
      "type": [
        "DocumentVerification"
      ],
      "verifier": "La Poste - Docaposte"
    }
  ]
}